  typedef enum tagBLDPROMPTOPT
  {
    BLDPROMPTOPT_PROMPTIFMULTIPLE = 0,
    BLDPROMPTOPT_PROMPTALWAYS     = 1,
    BLDPROMPTOPT_PROMPTNEVER      = 2,
  } BLDPROMPTOPT;

