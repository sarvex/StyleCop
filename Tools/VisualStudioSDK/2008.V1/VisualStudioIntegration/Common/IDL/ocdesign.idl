                       [in] DWORD grfGetOpt,
                       [in] HWND hwndPromptOwner,
                       [out] IDispatch **ppdispApp,
                       [out] HWND *pwndBuilderOwner,
                       [in] REFIID riidBuilder,
                       [out] IUnknown **ppunkBuilder);
    HRESULT EnableModeless([in] BOOL fEnable);
  }

